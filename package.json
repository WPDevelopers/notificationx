{
  "name": "notificationx",
  "version": "2.0.0",
  "main": "nxdev/index.js",
  "author": "WPDeveloper",
  "scripts": {
    "admin": "wp-scripts build",
    "admin-watch": "wp-scripts start",
    "frontend": "wp-scripts build --config webpack.frontend.config.js --webpack-no-externals",
    "frontend-watch": "wp-scripts start --config webpack.frontend.config.js --webpack-no-externals",
    "start": "npm run admin-watch & npm run frontend-watch",
    "build": "npm run admin && npm run frontend",
    "up": "npm install github:WPDevelopers/quickbuilder#dev",
    "pot": "wp i18n make-pot . languages/notificationx.pot ",
    "bb": "wp-scripts build --config webpack.blocks.config.js",
    "blocks": "wp-scripts start --config webpack.blocks.config.js",
    "release": "npm run build && npm run bb && npm run pot"
  },
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties"
  ],
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@types/react": "^17.0.19",
    "@types/react-router-dom": "^5.1.8",
    "@wordpress/api-fetch": "^5.2.1",
    "@wordpress/components": "^16.0.0",
    "@wordpress/date": "^4.2.1",
    "@wordpress/dom-ready": "^3.2.1",
    "@wordpress/hooks": "^3.2.0",
    "@wordpress/i18n": "^4.2.1",
    "@wordpress/media-utils": "^3.4.0",
    "@wordpress/scripts": "^18.0.0",
    "mini-css-extract-plugin": "2.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "apexcharts": "^3.28.1",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "delegate": "^3.2.0",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "html-react-parser": "^1.4.11",
    "html-to-draftjs": "^1.5.0",
    "moment": "^2.29.4",
    "quickbuilder": "github:WPDevelopers/quickbuilder#dev",
    "rc-pagination": "^3.1.13",
    "react-apexcharts": "^1.3.9",
    "react-cookies": "^0.1.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-draft-wysiwyg": "^1.14.7",
    "react-router-dom": "^5.3.1",
    "react-select": "^4.3.1",
    "react-toastify": "^8.0.3",
    "sort-array": "^4.1.4",
    "sweetalert2": "11.4.8"
  }
}
